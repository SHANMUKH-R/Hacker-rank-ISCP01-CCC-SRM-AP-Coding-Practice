#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    char str[101];
    scanf("%s",str);
    float count=0;
    int l=strlen(str);
    
    for(int i=0;str[i]!='\0';i++)
    {
        if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')   
        {
            count++;
        }
    }
    float percent=(count/l)*100;
    printf("%.4f",percent);
    return 0;
}
