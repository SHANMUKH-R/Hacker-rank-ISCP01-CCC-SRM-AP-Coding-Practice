#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N, i, j;
    scanf("%d",&N);
    if(N>=1 && N<=100){
        for(i=1;i<=N;i++){
            for(j=1;j<=i;j++){
                printf("%d",j);
            }
            printf("\n");
        }
    }
    return 0;
}
