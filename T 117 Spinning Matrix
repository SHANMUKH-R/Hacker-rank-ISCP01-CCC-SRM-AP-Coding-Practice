#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,s =0;
    scanf("%d",&n);
    int a[n][n];
    for(int i =0;i<n;i++){
        for(int j=0;j<n;j++){
             scanf("%d",&a[i][j]);
        }
    }
    while(1){
        char c;
        scanf("%c",&c);
        if(c == '-')
            return 0;
        else if(c == 'A'){
            int x;
            scanf("%d",&x);
            s=s+x/90;
            s=s%4;  
        }
         else if(c == 'Q'){
            int x,y;
            scanf("%d%d",&x,&y);
            x--;y--;
            if(s == 0)  printf("%d\n",a[x][y]);
            if(s== 1) printf("%d\n",a[n-y-1][x]);
            if(s== 2) printf("%d\n",a[n-x-1][n-y-1]);
            if(s== 3) printf("%d\n",a[y][n-x-1]);
        }
        else if(c == 'U')
        {
            int x,y,z;
            scanf("%d%d%d",&x,&y,&z);
            a[x-1][y-1]=z;
        } 
        
    }
   return 0;
}
