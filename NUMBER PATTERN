#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N, i, j,k;
    scanf("%d",&N);
    if(N>=1 && N<=100){
        for(i=1;i<=N;i++){
            for(j=1;j<=N-i;j++){
                printf(" ");
            }
            for(k=1;k<=i;k++){
                printf("%d",k);
            }
            printf("\n");
        }
    }
    return 0;
}
